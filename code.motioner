size(1280, 720)
timeline(fps = 60, duration = 5.00)

circle "Circle" {
x = 0.100,
y = 0.250,
radius = 0.080,
fill = "#78c8ff",
spawn = 0.00
}

on_time {
	let name = "Circle____"+frame
	let position = seconds*0.1
	circle name {
		x = position,
		y = 0.5,
		radius = 0.08,
		fill = "#39ff0d"
		spawn = seconds
	}
}