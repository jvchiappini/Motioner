    x = 0.500,
    y = 0.500,
    value = "Hello",
    font = "System",
    size = 24.0,
    fill = "#ffffff",
    spawn = 0.00,
}

move {
    element = "Circle",
    to = (1.000, 1.000),
    during = 0.000 -> 5.000,
    ease = linear
}

