size(1280, 720)
timeline(fps = 60, duration = 5.00)

circle "Circle" {
    x = 0.000,
    y = 0.000,
    radius = 0.100,
    fill = "#401235",
    spawn = 0.00
}
move {
    to = (1.000, 1.000),
    during = 0.000 -> 5.000,
    ease = linear
}


